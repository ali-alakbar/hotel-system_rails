
<div class="new_room_page">
    <div class="intro" style = "text-align: center;">
        <h1>Update The Room</h1>
    </div>
    
    <%= form_with(model: @room, local: true) do |form| %>
    <%= form.label "Price: " %>
    <%= form.text_field :total_price %>
    <br>

    <%= form.submit %>
    <% end %>

    <%= link_to "Back", rooms_path %>

</div>